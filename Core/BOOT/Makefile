all: OUT/MBR.BIN OUT/DBRFAT12.BIN OUT/DBRFAT16.BIN OUT/DBRFAT32.BIN

OUT/DBRFAT12.BIN: DBRFAT12.ASM
	nasm DBRFAT12.ASM -o OUT/DBRFAT12.BIN

OUT/DBRFAT16.BIN: DBRFAT16.ASM
	nasm DBRFAT16.ASM -o OUT/DBRFAT16.BIN

OUT/DBRFAT32.BIN: DBRFAT32.ASM
	nasm DBRFAT32.ASM -o OUT/DBRFAT32.BIN

OUT/MBR.BIN: MBR.ASM
	nasm MBR.ASM -o OUT/MBR.BIN

clean:
	del out\*.BIN
