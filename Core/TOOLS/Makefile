all: OUT\EXE2BIN.EXE OUT\MAKEFLP.EXE OUT\GETKEY1.COM OUT\GETKEY2.COM OUT\INSTMBR.COM

OUT\EXE2BIN.EXE: EXE2BIN\Entry.c
	gcc -O2 EXE2BIN\Entry.c -o OUT\EXE2BIN.EXE

OUT\MAKEFLP.EXE: MAKEFLP\Entry.c MAKEFLP\fat12img.c MAKEFLP\fat12img.h
	gcc -O2 MAKEFLP\Entry.c MAKEFLP\fat12img.c -o OUT\MAKEFLP.EXE

OUT\GETKEY1.COM: GETKEY1.ASM
	nasm GETKEY1.ASM -o OUT\GETKEY1.COM

OUT\GETKEY2.COM: GETKEY2.ASM
	nasm GETKEY2.ASM -o OUT\GETKEY2.COM

OUT\INSTMBR.COM: INSTMBR.ASM
	nasm INSTMBR.ASM -o OUT\INSTMBR.COM

clean:
	del OUT\*.EXE
	del OUT\*.COM
